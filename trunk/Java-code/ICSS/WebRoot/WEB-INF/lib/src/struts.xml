<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="ICSS" extends='struts-default' namespace="/myfolder">

		<action name="Reg_ajax_Action" class="reg.Reg_ajax_Action">
			<result name="input">/myfolder/reg.jsp</result>
			<result name="success">../success.jsp</result>
			<result name="error">/RegError.jsp</result>
		</action>


		<action name="search_action"
			class="icss.abc.search.ctrl.SearchAction">
			<result name="success">/myfolder/sousuojiegou.jsp</result>
			<result name="error">/error.jsp</result>
		</action>


		<action name="searchresult_action"
			class="icss.abc.search.ctrl.SearchresultAction">
			<result name="success">/myfolder/sousuojiegou.jsp</result>
			<result name="error">/error.jsp</result>
		</action>


		<action name="page_action"
			class="icss.abc.search.ctrl.PageAction">
			<result name="success">/myfolder/sousuojiegou.jsp</result>
		</action>


		<action name="supersearch_action"
			class="icss.abc.search.ctrl.SuperSearchAction">
			<result name="success">/myfolder/sousuojiegou.jsp</result>
			<result name="error">../error.jsp</result>
		</action>

		<action name="makeindex_action"
			class="icss.abc.search.ctrl.MakeIndexAction">
			<result name="success">../success.jsp</result>
		</action>
		
		
	</package>
</struts>
